{
  "name": "link-syntax",
  "displayName": "Link Syntax",
  "description": "Link syntax highlight support",
  "publisher": "katepark",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyutekrap/link-syntax.git"
  },
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "link",
      "aliases": ["Link", "link"],
      "extensions": [".link"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "link",
      "scopeName": "main.link",
      "path": "./syntaxes/link.tmLanguage.json"
    }]
  }
}
